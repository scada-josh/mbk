$("#knobViewEvent").knob({
		'readOnly': true,
	    'fgColor': "#00ABC6",
	    'bgColor': "#666666",
	    'thickness': 0.2,
	    'angleArc': 180,
	    'angleOffset': -90,
		'format' : function (value) {
			//console.log(value);
			return value + '%';
		}
});


$( window ).resize(function() {
    $("#knobViewEvent").redrawKnob();
});






