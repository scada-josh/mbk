function fnBreadcrumb_partials_hide() {
	$('.events-partials-breadcrumb-eventLink span.eventLink').text('Events');
	$('.events-partials-breadcrumb-angleRight').fadeOut().css('display', 'none');
	$('.events-partials-breadcrumb-titlePartial').fadeOut().css('display', 'none');
	$('.events-partials-breadcrumb-titlePartial').text('');
}